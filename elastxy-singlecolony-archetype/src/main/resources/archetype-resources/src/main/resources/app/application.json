{
	"appName" : "${artifactId}",
	
	"targetBuilder": {
		"type" : "java",
		"content" : "${groupId}.singlecolony.AppTargetBuilder"
	},
	
	"envFactory": {
		"type" : "java",
		"content" : "org.elastxy.core.engine.factory.StandardEnvFactory"
	},
	
	"genomaProvider": {
		"type" : "java",
		"content" : "${groupId}.singlecolony.AppGenomaProvider"
	},
	
	"alleleGenerator": {
		"type" : "java",
		"content" : "org.elastxy.core.engine.metadata.MetadataAlleleGenerator"
	},
	
	"populationFactory": {
		"type" : "java",
		"content" : "org.elastxy.core.engine.factory.StandardPopulationFactory"
	},
	
	"solutionsFactory": {
		"type" : "java",
		"content" : "org.elastxy.core.engine.metadata.MetadataSolutionsFactory"
	},
	
	"fitnessCalculator": {
		"type" : "java",
		"content" : "${groupId}.singlecolony.AppFitnessCalculator"
